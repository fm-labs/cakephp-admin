{
    "name": "fm-labs/cakephp-backend",
    "description": "Backend plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4",
        "fm-labs/cakephp-bootstrap": "dev-master"
    },
    "require-dev": {
        "cakephp/cakephp": "~3.0",
        "phpunit/phpunit": "<6.0"
    },
    "autoload": {
        "psr-4": {
            "Backend\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backend\\Test\\": "tests",
            "Cake\\": "./vendor/cakephp/cakephp/src",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
