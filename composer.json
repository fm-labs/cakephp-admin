{
    "name": "fm-labs/cakephp3-plugin-backend",
    "description": "Backend plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4",
        "cakephp/cakephp": "3.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Backend\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backend\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
