{
    "name": "fm-labs/cakephp-admin",
    "description": "Admin plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=8.2",
        "cakephp/cakephp": "^5",
        "fm-labs/cakephp-cupcake": "^5",
        "fm-labs/cakephp-bootstrap": "^5",
        "fm-labs/cakephp-sugar": "^5",
        "fm-labs/cakephp-user": "^5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    },
    "autoload": {
        "psr-4": {
            "Admin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Admin\\Test\\": "tests",
            "Cake\\": "./vendor/cakephp/cakephp/src",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
            "Cupcake\\": "./vendor/fm-labs/cakephp-cupcake/src",
            "Cupcake\\Test\\": "./vendor/fm-labs/cakephp-cupcake/tests"
        }
    }
}
